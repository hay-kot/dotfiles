{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffc107",
          "style": "diamond",
          "template": "{{ if .SSHSession }} {{ .UserName }}@{{ .HostName }} {{ end }}",
          "type": "session"
        },
        {
          "background": "#18354c",
          "foreground": "#ffc107",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#18354c",
          "foreground": "#ffc107",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#ffc107",
          "foreground": "#18354c",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#190649",
          "foreground": "#ffc107",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue77f {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "background": "#ffc107",
          "foreground": "#18354c",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#1e6679",
          "foreground": "#dddddd",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ufcd1 {{ .Full }} ",
          "type": "go"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#18354c",
          "foreground": "#AEA4BF",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "austin",
            "threshold": 150
          },
          "style": "diamond",
          "template": "{{ .FormattedMs }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "executiontime"
        },
        {
          "foreground": "#ffc107",
          "style": "diamond",
          "template": " \u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}