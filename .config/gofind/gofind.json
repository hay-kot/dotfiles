{
  "default": "repos",
  "commands": {
    "notebooks": {
      "roots": [
        "~/Code/notebook",
        "~/Library/Mobile Documents/iCloud~md~obsidian/Documents"
      ],
      "match": ".obsidian"
    },
    "repos": {
      "roots": [
        "~/Code/haybytes",
        "~/Code/Repos",
        "~/Code/pr",
        "~/Code/business"
      ],
      "match": ".git"
    }
  },
  "cache": "~/.cache/gofind/",
  "theme": {
    "prompt": "#7aa2f7",
    "selected_indicator": "#f7768e",
    "secondary_text": "#545c7e",
    "primary_text": "#c0caf5",
    "selection_background": "#283457"
  }
}
